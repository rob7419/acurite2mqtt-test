{
  "name": "Acurite to MQTT - TEST VERSION",
  "version": "0.3.27_test",
  "slug": "acurite2mqtt-test",
  "description": "TEST - Acurite Sensors to Home Assistant via MQTT with Autodiscovery_rob7419_version",
  "url": "https://github.com/rob7419/rob7419_acurite2mqtt_addons.git",
  "startup": "services",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "usb": true,
  "hassio_api": true,
  "hassio_role": "default",
  "auth_api": true,
  "ingress": false,
  "services": ["mqtt:want"],
  "discovery": ["mqtt"],
  "options":
    {
    "mqtt_host": "",
    "mqtt_port": 1883,
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_topic": "rtl_433",
    "mqtt_retain" : true,
    "protocol": "-R 11 -R 40 -R 41 -R 55 -R 74",
    "whitelist_enable": false,
    "whitelist": "",
    "expire_after": 0,
    "units": "si",
    "discovery_prefix": "homeassistant",
    "discovery_interval": 600,
    "auto_discovery": true,
    "debug": false
  },
  "schema":
    {
    "mqtt_host": "str?",
    "mqtt_port": "int",
    "mqtt_user": "str?",
    "mqtt_password": "password?",
    "mqtt_topic": "str",
    "mqtt_retain" : "bool",
    "protocol": "str",
    "whitelist_enable": "bool",
    "whitelist": "str",
    "expire_after": "int",
    "units": "str",
    "discovery_prefix": "str",
    "discovery_interval": "int",
    "auto_discovery": "bool",
    "debug": "bool"
   }
}
